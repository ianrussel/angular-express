{
	"name": "angular2-full-stack",
	"version": "4.2.4",
	"license": "MIT",
	"author": "Ian Russel Adem",
	"description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
	"angular-cli": {},
	"scripts": {
		"ng": "ng",
		"build": "ng build",
		"shit": "mkdirp dist/server/uploads",
		"start": "node dist/server/app.js",
		"predev": "tsc -p server",
		"dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
		"prod": "concurrently \"mongod\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
		"test": "ng test",
		"testbe": "tsc -p server && mocha dist/server/test --exit",
		"lint": "ng lint",
		"lintbe": "tslint server/**/**.ts{,x}",
		"e2e": "ng e2e",
		"postinstall": "tsc -p server && ng build -aot -prod && mkdirp dist/server/uploads"
	},
	"private": true,
	"dependencies": {
		"@angular/cli": "^1.4.9",
		"@angular/common": "^5.1.0",
		"@angular/compiler": "^5.1.0",
